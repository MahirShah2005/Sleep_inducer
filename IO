};
void Read(string name)
{
ifstream file(name);

if(!file){
    cerr<<"File cannot be opened"<< endl;
    exit(1);
}
string fline;
while(getline(file,fline)){
    string type;
    file >> type;
    cout << type;
    }
}




int main()
{   vector<Inmate> inmates;
    vector<Dorm> dorms;
    Read("input.txt");
    return 0;
}

